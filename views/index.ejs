<!doctype html>
<html lang="en">

    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible">
        <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
        <link href="css/app.css" rel="stylesheet"/>
        <title><%=pageTitle%></title>
    </head>

    <body id="<%=pageId%>">
        <header>    
            <div class="logo align-center">
            <img src="./images/logo_768.png" alt="Showcase2016"></a>
            </div>    
        </header>
        <main>
            <article>
                <section>
                    <h1>Sessions</h1>
                    <br>
                    <table>
                        <thead>
                        <tr>
                            <th>Time</th>
                            <th>Title</th>
                        </tr>
                        </thead>
                        <tbody id="sessionsTableContent">

                            <% if(sessionsTimeTable.length>0) { %>
                                <% console.log("### sessionsTimeTable.length: " + sessionsTimeTable.length); %>
                                <% for(row in sessionsTimeTable) { %>
                                    <tr>
                                        <td class="title"><%= sessionsTimeTable[row].time %></td><td><%= sessionsTimeTable[row].title %></td>
                                    </tr>
                                <% } %>
                            <% } %>

                        </tbody>
                    </table>
                </section>
            </article>
        </main>
    </body>

    <script src="js/app.js"></script>
</html>